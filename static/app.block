                    :hide_editors
                        {
                            //setTimeout(_ => {
                                block.child('subcontent/project_info/name/value_editor').on('hide');
                                block.child('subcontent/project_info/slug/value_editor').on('hide');
                                block.child('subcontent/project_info/tagline/value_editor').on('hide');
