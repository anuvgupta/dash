                                if (link_alt.length > length_limit) link_alt = link_alt.substring(0, length_limit - 2) + '...';
                            }
                            block.child('subcontent/resource_info/link/value/text').html(`${link_alt}`).attribute('href', `${link_alt_alt}`);
