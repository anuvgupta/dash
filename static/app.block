                                update_proxy = true;
                            }
                            if (old_data.proxy.ws_enable != new_data.proxy.ws_enable) {
                                update_proxy = true;
                            }
                            if (old_data.proxy.ws_enable_path != new_data.proxy.ws_enable_path) {
                                update_proxy = true;
                                block.child('subcontent/application_proxy/ws_enable_path/value_editor').on('hide');
